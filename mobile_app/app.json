{
  "expo": {
    "name": "LLM Chat Mobile",
    "slug": "llm-chat-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "name": "LLM Chat",
      "shortName": "LLM Chat",
      "description": "Mobile and web interface for LLM Chat API",
      "backgroundColor": "#ffffff",
      "themeColor": "#2196F3",
      "preferRelatedApplications": false
    },
    "platforms": ["ios", "android", "web"]
  }
}